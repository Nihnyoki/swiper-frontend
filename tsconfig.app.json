{
    "extends": "./tsconfig.json",
    "compilerOptions": {
  "module": "ESNext",
    "target": "ES2021",
    "jsx": "react-jsx",
    "moduleResolution": "bundler",
      "strictNullChecks": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "types": ["vite/client", "vite-plugin-pwa/client"],
      //"moduleResolution": "node",
      "baseUrl": ".",
      "paths": {
        "@/*": ["src/*"]
      }
    }
  }